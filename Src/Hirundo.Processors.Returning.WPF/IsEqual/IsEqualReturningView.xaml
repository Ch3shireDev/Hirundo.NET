<UserControl x:Class="Hirundo.Processors.Returning.WPF.IsEqual.IsEqualReturningView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Hirundo.Processors.Returning.WPF.IsEqual"
             xmlns:labels="clr-namespace:Hirundo.Commons.WPF.Repositories.Labels;assembly=Hirundo.Commons.WPF"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800"
             d:DataContext="{d:DesignInstance local:IsEqualReturningViewModel}">
    <StackPanel>
        <WrapPanel>
            <Label>Nazwa pola</Label>
            <labels:LabelsComboBox
                ValueName="{Binding ValueName}"
                DataType="{Binding DataType}"
                Repository="{Binding Repository}" />
        </WrapPanel>
        <WrapPanel>
            <Label>Wartość</Label>
            <TextBox Text="{Binding Value}" />
        </WrapPanel>
    </StackPanel>
</UserControl>